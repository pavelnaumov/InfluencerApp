<h1>Influencers#index</h1>

<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-md-8 col-sm-offset-1 col-md-offset-2">
      <% @influencers.each do |influencer| %>
      <ul class="list-group">
        <li class="list-group-item"><%= influencer.username %> <%= link_to "Show", influencer_path(influencer), class: "btn btn-primary" %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
